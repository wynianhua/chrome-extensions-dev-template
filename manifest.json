{
  "name": "多花调试扩展",
  "version": "1.2",
  "manifest_version": 2,
  "author": "bohrzhang",
  "description": "Separate Domains and Parameters and Regroup",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "activeTab",
    "tabs",
    "cookies",
    "https://*.pingan.com.cn/",
    "https://*.sdb.com.cn/"
  ],
  "browser_action": {
    "default_popup": "src/html/popup.html",
    "default_icon": "images/nokia_suite_mirror.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "./contentScript/dh.js"
      ]
    }
  ],
  "background": {
    "scripts": ["./background/cdn-huidu.js"],
    "persistent": true
  },
  "icons": {
    "16": "images/nokia_suite_mirror.png",
    "32": "images/nokia_suite_mirror.png",
    "48": "images/nokia_suite_mirror.png",
    "128": "images/nokia_suite_mirror.png"
  }
}